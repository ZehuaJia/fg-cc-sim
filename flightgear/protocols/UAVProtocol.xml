<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
  <generic>
    <output>
      <!--
      <var_separator>tab</var_separator>
      <line_separator>newline</line_separator>
      -->
      <binary_mode>true</binary_mode> 
      <byte_order>host</byte_order>
      <!-- 0: Time -->   
      <chunk>
        <name>time</name>
        <node>/position/timetoday</node>
        <type>float</type>
        <format>%.10f</format>
      </chunk>
      <!-- 1: x position -->
      <chunk>
        <name>posx</name>
        <node>/fdm/jsbsim/positioning/posx</node>
        <type>float</type>
        <format>%03.3f</format>
      </chunk>
      <!-- 1: y position -->
      <chunk>
        <name>posy</name>
        <node>/fdm/jsbsim/positioning/posy</node>
        <type>float</type>
        <format>%03.3f</format>
      </chunk>
      <!-- 3: altitude (ft)-->
      <chunk>
        <name>altitude</name>
        <node>/fdm/jsbsim/position/h-agl-ft</node>
        <type>float</type>
        <format>%03.4f</format>
      </chunk>
      <!-- 4: true airspeed -->
      <chunk>
        <name>airspeed</name>
        <node>/fdm/jsbsim/velocities/vt-fps</node>
        <!--<node>/fdm/jsbsim/velocities/vg-fps</node>-->
        <!--<node>/fdm/jsbsim/velocities/u-fps</node>-->
        <!--<node>/velocities/uBody-fps</node>-->
        <type>float</type>
        <format>%03.3f</format>
      </chunk>
      <!-- 5: acceleration -->
      <chunk>
        <name>acceleration</name>
        <node>/fdm/jsbsim/accelerations/udot-ft_sec2</node>
        <type>float</type>
        <format>%03.3f</format>
      </chunk>
      <!-- 6: path angle (rad)-->
      <chunk>
        <name>path_angle</name>
        <node>/fdm/jsbsim/flight-path/gamma-rad</node>
        <type>float</type>
        <format>%03.4f</format>
      </chunk>
      <!-- 7: heading angle (rad)-->
      <chunk>
        <name>heading_angle</name>
        <node>/fdm/jsbsim/flight-path/psi-gt-rad</node>
        <type>float</type>
        <format>%03.4f</format>
      </chunk>
      <!-- 8: roll angle (rad)-->
      <chunk>
        <name>roll_angle</name>
        <node>/fdm/jsbsim/attitude/phi-rad</node>
        <type>float</type>
        <format>%03.4f</format>
      </chunk>
    </output>
  </generic>
</PropertyList>
