<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
  <generic>
    <output>
      <!--
      <line_separator>newline</line_separator>
      <var_separator>tab</var_separator>
    -->
    <binary_mode>true</binary_mode> 
    <byte_order>host</byte_order>
    
    <!-- 0: Time -->
    <chunk>
      <name>Time</name>
      <node>/position/timetoday</node>
      <type>float</type>
      <format>%.10f</format>
    </chunk>

    <!-- 1: x position -->
    <chunk>
      <name>posx</name>
      <node>/fdm/jsbsim/positioning/posx</node>
      <type>float</type>
      <format>%03.3f</format>
    </chunk>

    <!-- 2: y position -->
    <chunk>
      <name>posy</name>
      <node>/fdm/jsbsim/positioning/posy</node>
      <type>float</type>
      <format>%03.3f</format>
    </chunk>

    <!-- 3: altitude (ft)-->
    <chunk>
      <name>altitude</name>
      <node>/fdm/jsbsim/position/h-agl-ft</node>
      <type>float</type>
      <format>%03.4f</format>
    </chunk>
    <!-- 4-6: true airspeed -->
    <chunk>
      <name>u</name>
      <node>/fdm/jsbsim/velocities/u-fps</node>
      <type>float</type>
      <format>%03.3f</format>
    </chunk>
    <chunk>
      <name>v</name>
      <node>/fdm/jsbsim/velocities/v-fps</node>
      <type>float</type>
      <format>%03.3f</format>
    </chunk>
    <chunk>
      <name>w</name>
      <node>/fdm/jsbsim/velocities/w-fps</node>
      <type>float</type>
      <format>%03.3f</format>
    </chunk>    
    <!-- 7-9: acceleration -->
    <chunk>
      <name>ax</name>
      <node>/fdm/jsbsim/accelerations/udot-ft_sec2</node>
      <type>float</type>
      <format>%03.3f</format>
    </chunk>
    <chunk>
      <name>ay</name>
      <node>/fdm/jsbsim/accelerations/vdot-ft_sec2</node>
      <type>float</type>
      <format>%03.3f</format>
    </chunk>
    <chunk>
      <name>az</name>
      <node>/fdm/jsbsim/accelerations/wdot-ft_sec2</node>
      <type>float</type>
      <format>%03.3f</format>
    </chunk>
    <!-- 10: heading angle (rad)-->
    <chunk>
      <name>heading</name>
      <node>/fdm/jsbsim/flight-path/psi-gt-rad</node>
      <type>float</type>
      <format>%03.4f</format>
    </chunk>
    <!-- 11:13 Attitude rates -->
      <chunk>
        <name>p</name>
        <node>/fdm/jsbsim/accelerations/p-rad_sec</node>
        <type>float</type>
      </chunk>
      <chunk>
        <name>q</name>
        <node>/fdm/jsbsim/accelerations/q-rad_sec</node>
        <type>float</type>
      </chunk>
      <chunk>
        <name>r</name>
        <node>/fdm/jsbsim/accelerations/r-rad_sec</node>
        <type>float</type>
      </chunk>          
    </output>
  </generic>
</PropertyList>
